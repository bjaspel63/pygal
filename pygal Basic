import pygal      #python library

pie_chart = pygal.Pie()    #type of chart
pie_chart.title = "Most Visited Website of 2023"  #title
pie_chart.x_labels = ['2023']

pie_chart.add("Google", 177.8)   #add data
pie_chart.add("Youtube", 117.3)
pie_chart.add("Facebook", 18.7)
pie_chart.add("X", 8.5)
pie_chart.add("Wikipedia", 18.4)

pie_chart.render()        #show chart
